{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' https://yourdomain.com;"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://yourdomain.com/"
          }
        ]
      }
    ]
  }
  